{
    "autoHideBehavior": {
        "enabled": true,
        "collapseDelay": 300,
        "collapseTime": 100,
        "expandTime": 100,
        "expandDelay": 300,
        "minArea": 5,
        "showOnWindowSwitch": true,
        "showOnWorkspaceSwitch": true
    },
    "fullWidth": false,
    "minWidth": 840,
    "maxWidth": 1600,
    "height": 50,
    "modules": {
        "left": [
            {
                "type": "workspaces",
                "config": {
                    "format": "{name}"
                }
            }
        ],
        "center": [
            {
                "type": "workspace-windows",
                "config": {
                    "format": "{name}"
                }
            }
        ],
        "right": [
            {
                "type": "clock",
                "config": {
                    "format": "hh:mm"
                }
            }
        ]
    }
}
